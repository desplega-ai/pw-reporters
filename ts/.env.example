# Desplega Reporter Configuration
# All values are optional - defaults work for local development

# API key for authentication
DESPLEGA_API_KEY=

# Base endpoint without protocol (e.g., 'localhost:5555' or 'api.desplega.ai')
# Default: localhost:5555
DESPLEGA_ENDPOINT=localhost:5555

# Use secure connections (wss/https)
# Default: false for localhost, true for other endpoints
DESPLEGA_SECURE=false

# Enable debug logging
# Default: false
DESPLEGA_DEBUG=false
